<div class="row">
  <div class="col-xs-12">
    <% if @model == "user" %>
      <h2>Users search for "<%= "#{@content}" %>"</h2>
        <%= render 'users/index', users: @datas %>
    <% elsif @model == "book" %>
      <h2>Books search for "<%= "#{@content}" %>"</h2>
        <%= render 'books/index', books: @datas %>
    <% end %>
  </div>
</div>
